    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-live": "^4.1.7",
    "styled-components": "^6.1.8",
    "typeface-bitter": "^0.0.71",
    "typeface-open-sans": "^0.0.75"
  },
